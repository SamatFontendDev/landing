{
    "trailingComma": "none",
    "semi": false,
    "tabWidth": 4,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^providers/(.*)$",
        "^constants/(.*)$",
        "^services/(.*)$",
        "^assets/(.*)$",
        "^routes/(.*)$",
        "^models/(.*)$",
        "^pages/(.*)$",
        "^store/(.*)$",
        "^types/(.*)$",
        "^com/(.*)$",
        "^../(.*)",
        "^./(.*)",
        "(.scss)$",
        "(.css)$"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}